{
  "name": "@agentconnect/cli",
  "version": "0.2.3",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/rayzhudev/agent-connect",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rayzhudev/agent-connect.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/rayzhudev/agent-connect/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "agentconnect": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run --cwd ../host build && tsc && esbuild src/index.ts --bundle --platform=node --format=esm --target=node20 --outfile=dist/index.js --banner:js='#!/usr/bin/env node' --packages=external",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@agentconnect/host": "^0.2.3",
    "ajv": "^8.17.1",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/yauzl": "^2",
    "@types/yazl": "^2",
    "typescript": "^5.6.2",
    "esbuild": "^0.23.0"
  },
  "engines": {
    "node": ">=20"
  }
}
